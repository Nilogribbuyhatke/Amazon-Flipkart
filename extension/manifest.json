{
  "manifest_version": 3,
  "name": "Task1",
  "version": "1.0",
  "permissions": ["storage", "activeTab"],
  "host_permissions": [
    "*://localhost/*",
    "*://*.myntra.com/*",
    "*://*.amazon.in/*"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.myntra.com/*", "*://*.amazon.in/*"],
      "js": ["content.js"]
    }
  ]
}